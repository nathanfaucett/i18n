{
  "name": "i18n",
  "version": "0.0.1",
  "description": "i18n for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/i18n.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "i18n",
    "i18n.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-jsbeautifier": "^0.2.10",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2",
    "zuul": "^3.7.2"
  },
  "dependencies": {
    "flatten_object": "git://github.com/nathanfaucett/flatten_object.git",
    "define_property": "git://github.com/nathanfaucett/define_property.git",
    "fast_slice": "git://github.com/nathanfaucett/fast_slice.git",
    "format": "git://github.com/nathanfaucett/format.git",
    "has": "git://github.com/nathanfaucett/has.git",
    "is_null": "git://github.com/nathanfaucett/is_null.git",
    "is_array": "git://github.com/nathanfaucett/is_array.git",
    "is_object": "git://github.com/nathanfaucett/is_object.git",
    "is_string": "git://github.com/nathanfaucett/is_string.git"
  }
}
